			<Anchor relative="$parent"/>
			<Texture val="#Tab2Img"/>
			<Visible val="False"/>
		</Frame>
		<Frame type="Image" name="Preload2">
			<Anchor relative="$parent"/>
			<Texture val="#Tab3Img"/>
			<Visible val="False"/>
		</Frame>
		<Frame type="Image" name="Preload3">
			<Anchor relative="$parent"/>
			<Texture val="#Tab4Img"/>
			<Visible val="False"/>
		</Frame>
		<Frame type="Image" name="Preload4">
